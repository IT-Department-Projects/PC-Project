   "source": [
    "from scipy import misc\n",
    "import numpy as np\n",
    "import scipy.misc\n",
    "\n",
    "arr = misc.imread('lena.png') # 640x480x3 array\n",
    "arr[20, 30] # 3-vector for a pixel\n",
    "arr[20, 30, 1] # green value for a pixel\n",
    "arr[20, 30, 1] = 200\n",
    "\n",
    "image = np.array(arr)\n",
    "\n",
    "\n",
    "\n",
    "scipy.misc.imsave('outfile.jpg', image)"
   ]
